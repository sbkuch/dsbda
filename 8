import pandas as pd
import numpy as np
%matplotlib inline
import matplotlib.pyplot as plt
import seaborn as sns

import pandas as pd
dataset = pd.read_csv(r"C:\Users\GAURI\OneDrive\Desktop\Titanic1.csv")

dataset.head()

plt.show()
sns.histplot(dataset['fare'])

sns.histplot(dataset['fare'], kde=False, bins=10)
plt.show()

sns.jointplot(x='age', y='fare', data=dataset)
plt.show()

sns.jointplot(x='age', y='fare', data=dataset, kind='hex')
plt.show()

dataset=dataset.dropna()

sns.pairplot(dataset)

sns.pairplot(dataset, hue='sex')

sns.rugplot(dataset['fare'])

sns.barplot(x='sex', y='age', data=dataset)
plt.show()

sns.barplot(x='sex', y='age', data=dataset, estimator=np.std)
plt.show()

sns.countplot(x='sex', data=dataset)
plt.show()

sns.boxplot(x='sex', y='age', data=dataset)
plt.show()

sns.boxplot(x='sex', y='age', data=dataset, hue="survived")
plt.show()

sns.violinplot(x='sex', y='age', data=dataset)
plt.show()

sns.violinplot(x='sex', y='age', data=dataset, hue='survived')
plt.show()

sns.violinplot(x='sex', y='age', data=dataset, hue='survived', split=True)
plt.show()

sns.stripplot(x='sex', y='age', data=dataset)
plt.show()

sns.stripplot(x='sex', y='age', data=dataset, jitter=True)
plt.show()

sns.stripplot(x='sex', y='age', data=dataset, jitter=True, hue='survived')
plt.show()

sns.stripplot(x='sex',y='age',data=dataset,jitter=True,hue='survived')
plt.show()

sns.swarmplot(x='sex', y='age', data=dataset)
plt.show()

sns.swarmplot(x='sex', y='age', data=dataset, hue='survived')
plt.show()

sns.swarmplot(x='sex', y='age', data=dataset, hue='survived')
plt.show()

sns.violinplot(x='sex', y='age', data=dataset)
sns.swarmplot(x='sex', y='age', data=dataset, color='black')
plt.show()
